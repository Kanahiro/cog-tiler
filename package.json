    "esbuild": "^0.21.4",
    "maplibre-gl": "^4.7.0",
    "npm-run-all2": "^6.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "upng-js": "^2.1.0",
    "geotiff": "^2.1.3",
